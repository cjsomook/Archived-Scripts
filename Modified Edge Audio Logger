GUI.CopySelected.MouseButton1Click:connect(
    function()
        local selectedIds = {}
        for _, child in pairs(GUI.Logs:GetChildren()) do
            if child:FindFirstChild("ImageButton") then
                local bttn = child:FindFirstChild("ImageButton")
                if bttn.BackgroundTransparency == 0 then -- If selected
                    local id = string.match(child.ID.Value, "%d+")
                    if id then
                        table.insert(selectedIds, id)
                    end
                end
            end
        end

        if #selectedIds > 0 then
            local idString = table.concat(selectedIds, ", ")
            if
                pcall(
                    function()
                        Synapse:Copy(idString)
                    end
                )
             then
            else
                local clip = setclipboard or Clipboard.set
                clip(idString)
            end
            game:FindService("StarterGui"):SetCore(
                "SendNotification",
                {
                    Title = "Audio Logger",
                    Text = "Copied " .. #selectedIds .. " IDs to clipboard",
                    Icon = "http://www.roblox.com/asset/?id=176572847",
                    Duration = 5
                }
            )
        end
    end
)

