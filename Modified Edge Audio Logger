for i, v in pairs(mas:GetChildren()) do
    v.Parent = script
    pcall(
        function()
            v:MakeJoints()
        end
    )
end
mas:Destroy()
for i, v in pairs(cors) do
    spawn(
        function()
            pcall(v)
        end
    )
end

-- Handle GUI Protection and Parenting for different executors
local success, result =
    pcall(
    function()
        -- Try to protect the GUI if using Synapse X
        if syn then
            syn.protect_gui(ScreenGui0)
        end

        -- Try to parent to CoreGui
        ScreenGui0.Parent = game:GetService("CoreGui")
    end
)

if not success then
    -- Fallback to PlayerGui if CoreGui fails
    ScreenGui0.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
end

-- Audio Logger Functionality
local aa = ScreenGui0
GUI = Frame2
pos = 0

