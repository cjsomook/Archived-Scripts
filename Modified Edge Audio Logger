GUI.Info.Listen.MouseButton1Click:Connect(
    function()
        if GUI.Info.Listen.Text == "Listen" then
            local samplesound = Instance.new("Sound")
            samplesound.Parent =
                game:GetService("Players").LocalPlayer.Character or game:GetService("Players").LocalPlayer
            samplesound.Looped = true
            samplesound.SoundId = selectedaudio
            samplesound:Play()
            samplesound.Name = "SampleSound"
            samplesound.Volume = 5
            GUI.Info.Listen.Text = "Stop"
        else
            local plr = game:GetService("Players").LocalPlayer
            local char = plr.Character
            if char then
                for _, sound in pairs(char:GetChildren()) do
                    if sound.Name == "SampleSound" then
                        sound:Destroy()
                    end
                end
            end
            for _, sound in pairs(plr:GetChildren()) do
                if sound.Name == "SampleSound" then
                    sound:Destroy()
                end
            end
            GUI.Info.Listen.Text = "Listen"
        end
    end
)

